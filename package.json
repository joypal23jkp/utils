{
  "name": "@whoj/utils-monorepo",
  "description": "Collection of common JS / TS helpers/utils by @whoj",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "keywords": [
    "typescript",
    "ts",
    "starter"
  ],
  "scripts": {
    "lint": "eslint \"packages/**/*.{js,ts,tsx,vue}\" --fix",
    "prepare": "pnpm -r run stub",
    "build": "pnpm -r run build",
    "play": "vite --config ./playground/vite.config.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.11.2",
    "@types/throttle-debounce": "^5.0.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/ui": "^0.23.4",
    "@whoj/eslint-config": "^1.0.1",
    "@whoj/utils": "workspace:^",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "core-js": "^3.25.5",
    "cross-env": "^7.0.3",
    "dot-prop": "^7.2.0",
    "eslint": "8.22.0",
    "eslint-plugin-testing-library": "^5.7.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "throttle-debounce": "^5.0.0",
    "ts-toolbelt": "^9.6.0",
    "type-fest": "^2.19.0",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "utility-types": "^3.10.0",
    "vite": "^3.1.8",
    "vite-dts": "^1.0.4",
    "vitest": "^0.23.4",
    "vue": "^3.2.41"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@vue/compiler-sfc"
      ]
    }
  },
  "packageManager": "pnpm@7.13.5",
  "author": {
    "name": "Jonson B.",
    "email": "www.jbc.bd@gmail.com",
    "url": "https://who-jonson.github.io"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module"
}
