{
  "name": "@whoj/utils-monorepo",
  "description": "Collection of common JS / TS helpers/utils by @whoj",
  "version": "1.6.0",
  "private": true,
  "license": "MIT",
  "keywords": [
    "typescript",
    "ts",
    "starter"
  ],
  "scripts": {
    "lint": "eslint \"packages/**/*.{js,ts,tsx,vue}\" --fix",
    "prepare": "pnpm -r run stub",
    "build": "pnpm -r run build",
    "play": "vite --config ./playground/vite.config.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^18.15.5",
    "@types/throttle-debounce": "^5.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/ui": "^0.29.7",
    "@whoj/eslint-config": "^1.2.0",
    "@whoj/utils": "workspace:^",
    "bumpp": "^9.0.0",
    "c8": "^7.13.0",
    "change-case": "^4.1.2",
    "core-js": "^3.29.1",
    "cross-env": "^7.0.3",
    "dot-prop": "^7.2.0",
    "eslint": "8.36.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "scule": "^1.0.0",
    "throttle-debounce": "^5.0.0",
    "ts-toolbelt": "^9.6.0",
    "type-fest": "^3.6.1",
    "typescript": "^5.0.2",
    "unbuild": "^1.1.2",
    "utility-types": "^3.10.0",
    "vite": "^4.2.1",
    "vitest": "^0.29.7",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4.14"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@vue/compiler-sfc",
        "esbuild",
        "rollup"
      ]
    }
  },
  "packageManager": "pnpm@7.30.0",
  "author": {
    "name": "Jonson B.",
    "email": "www.jbc.bd@gmail.com",
    "url": "https://who-jonson.github.io"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module"
}
